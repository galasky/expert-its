<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <uses-sdk android:minSdkVersion="14" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.app.GetStopActivity"
            android:label="@string/title_activity_get_stop" >
        </activity>
        <activity
            android:name="com.example.app.IsServiceAvailableByDate"
            android:label="@string/title_activity_is_service_available_by_date" >
        </activity>
    </application>

</manifest>
